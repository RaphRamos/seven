<%
  if @event.temporary
    fee_partial_url = if !@event.first_event?
                        'returning_disclaimer'
                      elsif @event.event_service_id == 1
                        'skills_advice_disclaimer'
                      elsif @event.offshore?
                        'offshore_disclaimer'
                      else
                        'onshore_disclaimer'
                      end
%>
  $('#step-4-content').html("<%= escape_javascript(render partial: fee_partial_url, locals: {event_id: @event.id} ) %>");
<% else %>
  location.href = 'event/<%= @event.id%>'
<% end %> 